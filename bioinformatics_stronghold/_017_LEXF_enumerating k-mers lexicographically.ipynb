    "from itertools import combinations_with_replacement\n",
    "alphabet = 'A C G T'\n",
    "alphabet_joined = alphabet.replace(' ', '')\n",
    "length = 2\n",
    "\n",
    "for combo in combinations_with_replacement(alphabet_joined, length):\n",
    "    for char in combo:\n",
    "        print(char, end='')\n",
    "    print()"
